<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.UserLoginController">
   <children>
      <TextField fx:id="userField" layoutX="226.0" layoutY="141.0" />
      <TextField fx:id="passwordField" layoutX="226.0" layoutY="221.0" />
      <Label fx:id="userLabel" layoutX="102.0" layoutY="145.0" text="User Name" />
      <Label fx:id="passLabel" layoutX="102.0" layoutY="225.0" text="Password" />
      <Label fx:id="title" layoutX="226.0" layoutY="48.0" text="Please Login">
         <font>
            <Font size="27.0" />
         </font>
      </Label>
      <Button fx:id="loginButton" layoutX="169.0" layoutY="296.0" mnemonicParsing="false" onAction="#onLoginButtonPressed" text="Login" />
      <Button fx:id="exitButton" layoutX="407.0" layoutY="296.0" mnemonicParsing="false" onAction="#onExitPressed" text="Exit" />
      <Label fx:id="zoneIdLabel" layoutX="281.0" layoutY="103.0" text="zoneID" />
   </children>
</AnchorPane>
